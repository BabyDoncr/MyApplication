            android:exported="true" />
        <activity
            android:name="com.lkl.seniorui.ListViewBaseAdapterActivity"
            android:exported="true" />
        <activity
            android:name="com.lkl.seniorui.ListViewSimpleAdapterActivity"
            android:exported="true" />
        <activity
            android:name="com.lkl.seniorui.ListViewArrayAdapterActivity"
            android:exported="true" />
        <activity
            android:name="com.lkl.seniorui.SpinnerActivity"
            android:exported="true" />
        <activity
            android:name="com.lkl.seniorui.AutoComplateTextViewActivity"
            android:exported="true" />
        <activity
            android:name="com.lkl.seniorui.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>